{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended"],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": 12,
		"sourceType": "module"
	},
	"plugins": ["@typescript-eslint"],
	"rules": {
		"linebreak-style": 0,
		"quotes": ["error", "double"],
		"semi": ["error", "always"],
		"@typescript-eslint/no-explicit-any": 2
	}
}
